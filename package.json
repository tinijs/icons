{
  "name": "@tinijs/icons",
  "version": "0.16.0",
  "description": "The icon packages of the TiniJS framework.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:1:bootstrap && npm run build:2:material && npm run build:3:mdi && npm run build:4:ionic && npm run build:5:fluent && npm run build:6:ant && npm run build:7:fontawesome && npm run build:8:hero && npm run build:9:tabler && npm run build:10:lucide && npm run build:11:simple && npm run build:12:lineawesome && npm run build:13:cssgg && npm run build:14:box && npm run build:15:remix && npm run build:16:game",
    "build:1:bootstrap": "tini ui icon @tinijs/bootstrap-icons _/bootstrap-icons/icons",
    "preview:1:bootstrap": "parcel build/@tinijs/bootstrap-icons/index.html",
    "publish:1:bootstrap": "cd build/@tinijs/bootstrap-icons && npm publish --access public",
    "build:2:material": "tini ui icon @tinijs/material-icons _/@material-design-icons/svg:filled+,outlined+,round+,sharp+,two-tone+",
    "preview:2:material": "parcel build/@tinijs/material-icons/index.html",
    "publish:2:material": "cd build/@tinijs/material-icons && npm publish --access public",
    "build:3:mdi": "tini ui icon @tinijs/mdi-icons _/@mdi/svg/svg",
    "preview:3:mdi": "parcel build/@tinijs/mdi-icons/index.html",
    "publish:3:mdi": "cd build/@tinijs/mdi-icons && npm publish --access public",
    "build:4:ionic": "tini ui icon @tinijs/ionic-icons _/ionicons/dist/svg",
    "preview:4:ionic": "parcel build/@tinijs/ionic-icons/index.html",
    "publish:4:ionic": "cd build/@tinijs/ionic-icons && npm publish --access public",
    "build:5:fluent": "tini ui icon @tinijs/fluent-icons _/@fluentui/svg-icons/icons -h ./hooks/fluent.js",
    "preview:5:fluent": "parcel build/@tinijs/fluent-icons/index.html",
    "publish:5:fluent": "cd build/@tinijs/fluent-icons && npm publish --access public",
    "build:6:ant": "tini ui icon @tinijs/ant-icons _/@ant-design/icons-svg/inline-namespaced-svg:filled+,outlined+,twotone+",
    "preview:6:ant": "parcel build/@tinijs/ant-icons/index.html",
    "publish:6:ant": "cd build/@tinijs/ant-icons && npm publish --access public",
    "build:7:fontawesome": "tini ui icon @tinijs/fontawesome-icons ./downloads/Font-Awesome/svgs:solid,regular,brands",
    "preview:7:fontawesome": "parcel build/@tinijs/fontawesome-icons/index.html",
    "publish:7:fontawesome": "cd build/@tinijs/fontawesome-icons && npm publish --access public",
    "build:8:hero": "tini ui icon @tinijs/hero-icons _/heroicons/optimized/24:solid,outline",
    "preview:8:hero": "parcel build/@tinijs/hero-icons/index.html",
    "publish:8:hero": "cd build/@tinijs/hero-icons && npm publish --access public",
    "build:9:tabler": "tini ui icon @tinijs/tabler-icons _/@tabler/icons/icons",
    "preview:9:tabler": "parcel build/@tinijs/tabler-icons/index.html",
    "publish:9:tabler": "cd build/@tinijs/tabler-icons && npm publish --access public",
    "build:10:lucide": "tini ui icon @tinijs/lucide-icons _/lucide-static/icons",
    "preview:10:lucide": "parcel build/@tinijs/lucide-icons/index.html",
    "publish:10:lucide": "cd build/@tinijs/lucide-icons && npm publish --access public",
    "build:11:simple": "tini ui icon @tinijs/simple-icons _/simple-icons/icons",
    "preview:11:simple": "parcel build/@tinijs/simple-icons/index.html",
    "publish:11:simple": "cd build/@tinijs/simple-icons && npm publish --access public",
    "build:12:lineawesome": "tini ui icon @tinijs/lineawesome-icons _/line-awesome/svg",
    "preview:12:lineawesome": "parcel build/@tinijs/lineawesome-icons/index.html",
    "publish:12:lineawesome": "cd build/@tinijs/lineawesome-icons && npm publish --access public",
    "build:13:cssgg": "tini ui icon @tinijs/cssgg-icons _/css.gg/icons/svg",
    "preview:13:cssgg": "parcel build/@tinijs/cssgg-icons/index.html",
    "publish:13:cssgg": "cd build/@tinijs/cssgg-icons && npm publish --access public",
    "build:14:box": "tini ui icon @tinijs/box-icons _/boxicons/svg:logos,regular,solid+ -h ./hooks/box.js",
    "preview:14:box": "parcel build/@tinijs/box-icons/index.html",
    "publish:14:box": "cd build/@tinijs/box-icons && npm publish --access public",
    "build:15:remix": "tini ui icon @tinijs/remix-icons _/remixicon/icons",
    "preview:15:remix": "parcel build/@tinijs/remix-icons/index.html",
    "publish:15:remix": "cd build/@tinijs/remix-icons && npm publish --access public",
    "build:16:game": "tini ui icon @tinijs/game-icons ./downloads/Game-Icons/icons/000000/transparent/1x1",
    "preview:16:game": "parcel build/@tinijs/game-icons/index.html",
    "publish:16:game": "cd build/@tinijs/game-icons && npm publish --access public",
    "lint": "gts lint",
    "fix": "gts fix",
    "build:hooks": "tsc -p ./tsconfig.hooks.json",
    "download": "npm run download:fontawesome && npm run download:game",
    "download:fontawesome": "curl https://github.com/FortAwesome/Font-Awesome/archive/refs/tags/6.5.1.zip -L -o ./downloads/Font-Awesome.zip && unzip -o ./downloads/Font-Awesome.zip -d ./downloads && mv ./downloads/Font-Awesome-6.5.1 ./downloads/Font-Awesome && rm ./downloads/Font-Awesome.zip",
    "download:game": "curl https://game-icons.net/archives/svg/zip/000000/transparent/game-icons.net.svg.zip -L -o ./downloads/Game-Icons.zip && unzip -o ./downloads/Game-Icons.zip -d ./downloads/Game-Icons && rm ./downloads/Game-Icons.zip"
  },
  "keywords": [
    "TiniJS",
    "icons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tinijs/icons.git"
  },
  "bugs": {
    "url": "https://github.com/tinijs/icons/issues"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.4.2",
    "@fluentui/svg-icons": "^1.1.230",
    "@material-design-icons/svg": "^0.14.13",
    "@mdi/svg": "^7.4.47",
    "@tabler/icons": "^2.47.0",
    "bootstrap-icons": "^1.11.3",
    "boxicons": "^2.1.4",
    "css.gg": "^2.1.1",
    "heroicons": "https://github.com/tailwindlabs/heroicons/archive/refs/tags/v2.1.1.tar.gz",
    "ionicons": "^7.2.2",
    "line-awesome": "^1.3.0",
    "lucide-static": "^0.339.0",
    "remixicon": "^4.1.0",
    "simple-icons": "^11.6.0"
  },
  "devDependencies": {
    "@lit/react": "^1.0.3",
    "@tinijs/cli": "file:../cli/tinijs-cli-0.16.0.tgz",
    "@tinijs/core": "^0.16.0",
    "@types/node": "^20.11.20",
    "gts": "^5.2.0",
    "lit": "^3.1.2",
    "parcel": "^2.11.0",
    "react": "^18.2.0",
    "tinijs": "^0.16.0",
    "typescript": "~5.3.3"
  }
}
